package test;

import ex24.ui_code_seperation.Exam;

public class Test {

	public static void main(String[] args) {
		
		TjExam tj = new TjExam(2,3,4,5);

		int total = tj.total();
		float avg = tj.avg();

		System.out.printf("%d, %.2f\n", total, avg);
		
		Exam exam = new TjExam(1,1,1,1);
		total = exam.total();
		System.out.println(total);
		
		System.out.println(tj.toString());
	}

}
