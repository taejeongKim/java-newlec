package test;

import java.util.Scanner;

public class ExamList {

	private Exam[] exams;
	private int current;
	
	public ExamList() {
		this(3);
	}
	
	public ExamList(int size) {
		this.exams = new Exam[size];
		this.current= 0;
	}
	


	public void add(Exam exam) {
		exams[current] = exam;
		current++;
		
	}

	public Exam get(int i) {
		
		return exams[i];
	}

	public int size() {
		return current;
	}




}
