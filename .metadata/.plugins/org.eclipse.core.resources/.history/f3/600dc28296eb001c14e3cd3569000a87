package drawing;

public class Line extends Shape{

	private int length;
	
	public Line() {
//		super();
	}
	
	public Line(int x, int y) {
		super();
	}
	
	public int getLength() {
		return length;
	}

	public void setLength(int length) {
		this.length = length;
	}
	
	// getter & setter
	
	
}
