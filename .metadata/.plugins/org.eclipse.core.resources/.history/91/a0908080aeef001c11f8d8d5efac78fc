package ex25.inheritance;

public class NewlecExam extends Exam {

	private int com;

	public int getCom() {
		return com;
	}

	public void setCom(int com) {
		this.com = com;
	}
	
	@Override
	public int total() {
		// super : 부도의 객체
		return super.total() + com;
	}
	
	@Override
	public float avg() {
		// 아래는 this.total()이다.
		return total()/4.0f;
	}
}
