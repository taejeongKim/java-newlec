package newlecExam;

import ex25.abstraction.Exam;

public class NewlecExam extends Exam{

	private int com;
	
	public NewlecExam() {
		this(0,0,0,0);
	}
	// 오버로드 생성자
	public NewlecExam(int kor, int eng, int math, int com) {
		super(kor, eng, math);
		this.com = com;
	}
	public int getCom() {
		return com;
	}
	public void setCom(int com) {
		this.com = com;
	}
	
	// 부모클래스의 2개 함수 오버라이드
	
	@Override
	public int total() {
		return onTotal() + com;
	}
	
	@Override
	public float avg() {
		return total()/4.0f;
	}
	
	
}
