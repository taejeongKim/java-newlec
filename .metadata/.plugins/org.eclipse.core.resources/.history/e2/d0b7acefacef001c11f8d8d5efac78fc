package ex25.inheritance;
/*
 * 메소드를 오버라이드하지 않으면 아래 결과가 예상과 다르게 나온다.
 * */
public class Program {

	public static void main(String[] args) {
		
		NewlecExam exam = new NewlecExam();
		exam.setKor(10);
		exam.setEng(10);
		exam.setMath(10);
		exam.setCom(10);
		int total = exam.total();
		System.out.println(total);
	}

}
