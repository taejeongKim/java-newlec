package test;

public class TestProgram {


	public static void main(String[] args) {

/*국/영/수 성적관리 프로그램
 * 성적입력
 * 성적출력
 * 종료
 * */		
		
		boolean running = true;
		while(running) {
			System.out.println("--- 메뉴 ---");
			System.out.println("1) 성적입력");
			System.out.println("2) 성적입력");
			System.out.println("3) 종료");
			System.out.print(" > ");
			
			break;
			
		}
		
	}

}